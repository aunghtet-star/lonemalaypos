<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 32" width="32" height="32">
  <defs>
    <style>
      .restaurant-icon { fill: #4f46e5; }
      .restaurant-accent { fill: #10b981; }
    </style>
  </defs>

  <!-- Plate -->
  <circle cx="16" cy="20" r="8" class="restaurant-icon" fill-opacity="0.1" stroke="currentColor" stroke-width="1.5"/>
  <circle cx="16" cy="19" r="6" class="restaurant-icon"/>

  <!-- Food items on plate -->
  <ellipse cx="13" cy="17" rx="1.5" ry="1" class="restaurant-accent"/>
  <ellipse cx="16" cy="16" rx="1" ry="1.5" class="restaurant-accent"/>
  <ellipse cx="19" cy="18" rx="1.2" ry="0.8" class="restaurant-accent"/>

  <!-- Fork and knife -->
  <rect x="22" y="8" width="1" height="8" class="restaurant-icon"/>
  <rect x="24" y="8" width="1" height="8" class="restaurant-icon"/>
  <rect x="22" y="6" width="3" height="2" class="restaurant-icon" rx="1"/>

  <!-- Knife -->
  <polygon points="26,6 30,8 30,10 26,8" class="restaurant-accent"/>
  <rect x="26" y="8" width="1" height="6" class="restaurant-accent"/>
</svg>
